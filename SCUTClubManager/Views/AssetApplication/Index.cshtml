@model IEnumerable<SCUTClubManager.Models.AssetApplication>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            SubEvent
        </th>
        <th>
            Time
        </th>
        <th>
            Club
        </th>
        <th>
            Status
        </th>
        <th>
            Applicant
        </th>
        <th>
            Date
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SubEvent.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Time.TimeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Club.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Applicant.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
