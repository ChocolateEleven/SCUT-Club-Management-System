<!DOCTYPE html>
<html>
<head>
    <title>高校社团信息管理系统 @ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

    <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.2/themes/smoothness/jquery-ui.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/base/jquery-ui-timepicker-addon.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/base/navigationbar.css")" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-1.9.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-timepicker-addon.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/helpers.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.easing.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.easing.compatibility.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/navigationbar.js")" type="text/javascript"></script>

</head>
<body>
    <div id="upperBackgroundbar"></div>
    <div id="titlebar"></div>
    <div id="lowerBackgroundbar"></div>
    <div id="ribbon"></div>
    <div id="ribbonEnd">
        <img src="@Url.Content("~/Content/themes/base/images/ribbon_end.png")" />
    </div>
    <div class="page" style="position: relative">
        <div id="header">
            <div id="title">
                <h1>高校社团信息管理系统</h1>
            </div>
            <div id="navigationMenuRemains" class="NavigationMenuBackgroundColor">
                <span>&nbsp;</span>
            </div>
            <div class="Navigationbar">
                <ul class="Navigationbar">
                    <li class="NavigationbarItem NavigationMenuBackgroundColor">
                        <p class="NavigationbarItem">
                            <a href="/User/List" class="NavigationbarItem NotButton">用户</a>
                        </p>
                        <p class="NavigationbarSubItem">More info</p>
                    </li>
                    <li class="NavigationbarItem NavigationMenuBackgroundColor">
                        <p class="NavigationbarItem">
                            <a href="/Club" class="NavigationbarItem NotButton">社团</a>
                        </p>
                        <p class="NavigationbarSubItem">More info</p>
                    </li>
                    <li class="NavigationbarItem NavigationMenuBackgroundColor">
                        <p class="NavigationbarItem">
                            <a href="/Event" class="NavigationbarItem NotButton">活动</a>
                        </p>
                        <p class="NavigationbarSubItem">More info</p>
                    </li>
                    <li class="NavigationbarItem NavigationMenuBackgroundColor">
                        <p class="NavigationbarItem">
                            <a href="/Location" class="NavigationbarItem NotButton">场地</a>
                        </p>
                        <p class="NavigationbarSubItem">More info</p>
                    </li>
                    <li class="NavigationbarItem NavigationMenuBackgroundColor">
                        <p class="NavigationbarItem">
                            <a href="/Asset" class="NavigationbarItem NotButton">物资</a>
                        </p>
                        <p class="NavigationbarSubItem">More info</p>
                    </li>
                    <li class="NavigationbarItem NavigationMenuBackgroundColor">
                        <p class="NavigationbarItem">
                            <a href="/FundApplication" class="NavigationbarItem NotButton">资金</a>
                        </p>
                        <p class="NavigationbarSubItem">More info</p>
                    </li>
                    <li class="NavigationbarItem NavigationMenuBackgroundColor">
                        <p class="NavigationbarItem">
                            <a href="/Poll" class="NavigationbarItem NotButton">投票</a>
                        </p>
                        <p class="NavigationbarSubItem">More info</p>
                    </li>
                    <li class="NavigationbarItem NavigationMenuBackgroundColor">
                        <p class="NavigationbarItem">
                            <a href="/Forum" class="NavigationbarItem NotButton">留言</a>
                        </p>
                        <p class="NavigationbarSubItem">More info</p>
                    </li>
                    <li class="NavigationbarItem NavigationMenuBackgroundColor">
                        <p class="NavigationbarItem">
                            <a href="/Message" class="NavigationbarItem NotButton">消息</a>
                        </p>
                        <p class="NavigationbarSubItem">More info</p>
                    </li>
                </ul>
            </div>
        </div>
        <div id="main" style="position: absolute; width: 100%;">
            @RenderBody()
        </div>
        <div id="footer">
        </div>
    </div>
    <div id="logindisplay">
        @Html.Partial("_LogOnPartial")
    </div>
</body>

<script type="text/javascript">
    $(function () {
        $('input[type="file"]').each(function (index, element) {
            fileUploader(element, "选择文件", "未选择文件");
        });

        $('a:not(.NotButton)').button();
        $('button').button();
        $('input[type="submit"]').button();
        $(".Radio").buttonset();

        $('.display-label').addClass("ui-widget-header");
        $('.editor-label').addClass("ui-widget-header");
    })
</script>

</html>
